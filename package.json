{
  "name": "react-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "node clean.dev.js",
    "build": "yarn run clean && yarn run webpack --mode production",
    "start": "yarn run webpack serve"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
